<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.restaurant.birthdaywish">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@drawable/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="com.images.activities.FullStoryOfNews"></activity>
        <activity android:name="com.images.activities.ShowImageActivity" />
        <activity android:name="com.images.activities.topnews.NewsPageActivity" />
        <activity android:name="com.images.activities.LoveCalculatorActivity" />
        <activity android:name="com.images.activities.Covid19StatsActivity" />
        <activity android:name="com.images.activities.Covid19Activity" />
        <activity android:name="com.images.activities.MultiItemActivity"></activity>
        <activity android:name="com.images.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.images.GetNameActivity" />
        <activity android:name="com.images.activities.topnews.TopNewsActivity" />
        <activity android:name="com.images.activities.citynews.CityNewsActivity" />
        <activity android:name="com.images.ProfileImagesActivity" />
        <activity android:name="com.images.MainActivity" />
    </application>

</manifest>